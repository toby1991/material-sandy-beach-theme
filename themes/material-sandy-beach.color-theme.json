  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#888477",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#888477",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Tags",
      "scope": [
        "comment.block.documentation storage.type",
        "comment.block.documentation punctuation.definition.block.tag",
        "comment.block.documentation variable.other",
        "comment.block.documentation entity.name.type"
      ],
      "settings": {
        "foreground": "#945EB8",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Documentation Tag Values",
      "scope": [],
      "settings": {
        "foreground": "#546E7A"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#91B859"
      }
    },
    {
      "name": "String Escape Sequences",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#F76D47"
      }
    },
    {
      "name": "Invalid String Escape",
      "scope": [
        "invalid.illegal.string"
      ],
      "settings": {
        "foreground": "#E53935"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#F76D47"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#945EB8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#39ADB5"
      }
    },
    {
      "name": "Punctuation (Braces, Brackets, Parentheses)",
      "scope": [
        "punctuation.section.block",
        "punctuation.section.scope",
        "meta.brace",
        "punctuation.section.arguments",
        "punctuation.section.parameters",
        "punctuation.definition.parameters",
        "punctuation.section.array"
      ],
      "settings": {
        "foreground": "#39ADB5"
      }
    },
    {
      "name": "Punctuation (Comma, Semicolon, Dot)",
      "scope": [
        "punctuation.separator.comma",
        "punctuation.terminator.statement",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#39ADB5"
      }
    },
    {
      "name": "Function Declarations",
      "scope": [
        "entity.name.function",
        "meta.function entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6182B8"
      }
    },
    {
      "name": "Function Calls",
      "scope": [
        "meta.function-call entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6182B8"
      }
    },
    {
      "name": "Go Built-in Function Call",
      "scope": [
        "support.function.go"
      ],
      "settings": {
        "foreground": "#6182B8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class/Type Names",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.interface",
        "entity.name.enum",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#E2931D"
      }
    },
    {
      "name": "Interface Names",
      "scope": [
        "entity.name.interface"
      ],
      "settings": {
        "foreground": "#91B859"
      }
    },
    {
      "name": "Go Type Reference",
      "scope": [
        "support.type.go",
        "entity.name.type.go"
      ],
      "settings": {
        "foreground": "#91B859"
      }
    },
    {
      "name": "Go Package Name",
      "scope": [
        "entity.name.package.go"
      ],
      "settings": {
        "foreground": "#E2931D"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "meta.definition.variable entity.name.variable"
      ],
      "settings": {
        "foreground": "#272727"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#F76D47"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#F76D47"
      }
    },
    {
      "name": "Reassigned Variables/Parameters",
      "scope": [],
      "settings": {}
    },
    {
      "name": "Object Properties / Instance Fields",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.field",
        "meta.field.declaration entity.name.variable"
      ],
      "settings": {
        "foreground": "#272727"
      }
    },
    {
      "name": "Static Fields",
      "scope": [
        "variable.other.constant.object",
        "entity.name.variable.static"
      ],
      "settings": {
        "foreground": "#272727",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#E53935"
      }
    },
    {
      "name": "HTML/XML Tag Brackets",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#6DC2B8"
      }
    },
    {
      "name": "HTML/XML Attribute Names",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E2931D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML/XML Attribute Values",
      "scope": [
        "string.quoted.double",
        "string.quoted.single"
      ],
      "settings": {
        "foreground": "#91B859"
      }
    },
    {
      "name": "CSS Tag Name",
      "scope": [
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "CSS ID Selector",
      "scope": [
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#945EB8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Class Selector",
      "scope": [
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#E2931D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Property Name",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#8796B0"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.color.css",
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#F76D47"
      }
    },
    {
      "name": "CSS Function Call",
      "scope": [
        "support.function.misc.css"
      ],
      "settings": {
        "foreground": "#6182B8"
      }
    },
    {
      "name": "CSS !important",
      "scope": [
        "keyword.other.important.css"
      ],
      "settings": {
        "foreground": "#F76D47",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "entity.name.section.markdown",
        "markup.heading.setext"
      ],
      "settings": {
        "foreground": "#E53935",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#6182B8"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#39ADB5",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code Block/Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#91B859"
      }
    },
    {
      "name": "Invalid/Illegal",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E53935",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#888477",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
